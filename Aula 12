      "cell_type": "markdown",
      "source": [
        "# **3.2. BoxPlot**"
      ],
      "metadata": {
        "id": "nbcUHyyTafED"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "\n",
        "# Estilo semelhante ao ggplot2\n",
        "sns.set_theme(style=\"whitegrid\")\n",
        "\n",
        "# Dados do PIB em trilhões de dólares (US$)\n",
        "pib = [0.89, 1.10, 1.39, 1.65, 1.66, 2.21, 2.61, 2.46, 2.47, 2.41,\n",
        "       1.80, 1.80, 2.06, 1.92, 1.84, 1.44, 1.61, 1.92, 2.17, 2.25]\n",
        "\n",
        "# Cálculo dos quartis\n",
        "q1 = np.percentile(pib, 25)\n",
        "q2 = np.percentile(pib, 50)\n",
        "q3 = np.percentile(pib, 75)\n",
        "\n",
        "# Diferença interquartil\n",
        "iqr = q3 - q1\n",
        "\n",
        "# Impressão dos resultados\n",
        "print(f\"1º Quartil (Q1): {q1:.2f}\")\n",
        "print(f\"2º Quartil (Q2 ou mediana): {q2:.2f}\")\n",
        "print(f\"3º Quartil (Q3): {q3:.2f}\")\n",
        "print(f\"Intervalo Interquartil (IQR): {iqr:.2f}\")\n",
        "\n",
        "# Linhas auxiliares\n",
        "lower_bound = q1 - 1.5 * iqr\n",
        "upper_bound = q3 + 1.5 * iqr\n",
        "\n",
        "print(f\"Q1 - 1.5 × IQR = {lower_bound:.2f}\")\n",
        "print(f\"Q3 + 1.5 × IQR = {upper_bound:.2f}\")\n",
        "\n",
        "# Estilo semelhante ao ggplot2\n",
        "sns.set_theme(style=\"whitegrid\")\n",
        "\n",
        "# Criar o boxplot na vertical, sem anotações e sem o \"1\" no eixo x\n",
        "plt.figure(figsize=(4, 6))\n",
        "plt.boxplot(pib, vert=False, patch_artist=True,\n",
        "            boxprops=dict(facecolor='k', color='black'),\n",
        "            medianprops=dict(color='red'),\n",
        "            flierprops=dict(marker='o', markerfacecolor='orange', markersize=8, linestyle='none'))\n",
        "\n",
        "plt.title('Boxplot do PIB (em trilhões de dólares)')\n",
        "plt.xlabel('PIB (US$ trilhões)')\n",
        "plt.yticks([])  # Remove o \"1\" do eixo x\n",
        "plt.grid(axis='x')\n",
        "plt.tight_layout()\n",
        "plt.show()"
      ],
