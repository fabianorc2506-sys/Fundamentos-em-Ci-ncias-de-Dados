      "source": [
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "\n",
        "# Estilo semelhante ao ggplot2\n",
        "sns.set_theme(style=\"whitegrid\")\n",
        "\n",
        "# Dados do PIB em trilhões de dólares (US$)\n",
        "anos = list(range(2005, 2025))\n",
        "pib = [0.89, 1.10, 1.39, 1.65, 1.66, 2.21, 2.61, 2.46, 2.47, 2.41,\n",
        "       1.80, 1.80, 2.06, 1.92, 1.84, 1.44, 1.61, 1.92, 2.17, 2.25]\n",
        "\n",
        "# Criar histograma com densidade\n",
        "plt.figure(figsize=(8, 5))\n",
        "sns.histplot(pib, kde=True, stat=\"density\", color=\"skyblue\", edgecolor=\"black\")\n",
        "\n",
        "# Títulos e rótulos com estilo\n",
        "plt.title(\"Histograma com Densidade do PIB (2005–2024)\", fontsize=14, weight='bold')\n",
        "plt.xlabel(\"PIB (em trilhões de US$)\", fontsize=12)\n",
        "plt.ylabel(\"Densidade de Frequência\", fontsize=12)\n",
        "\n",
        "# Remover bordas desnecessárias (estilo ggplot2)\n",
        "sns.despine()\n",
        "\n",
        "# Mostrar gráfico\n",
        "plt.tight_layout()\n",
        "plt.show()"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "# Definição dos números de bins\n",
        "num_bins = [5, 9, 13, 31]\n",
        "\n",
        "# Estilo semelhante ao ggplot2\n",
        "sns.set_theme(style=\"whitegrid\")\n",
        "\n",
        "# Criar subgráficos\n",
        "fig, axes = plt.subplots(2, 2, figsize=(12, 10))\n",
        "\n",
        "for ax, bins in zip(axes.flatten(), num_bins):\n",
        "    sns.histplot(pib, bins=bins, kde=True, stat=\"density\", color=\"skyblue\", edgecolor=\"black\", ax=ax)\n",
        "    ax.set_title(f\"Histograma ({bins} Classes)\", fontsize=12, weight='bold')\n",
        "    ax.set_xlabel(\"PIB (em trilhões de US$)\", fontsize=10)\n",
        "    ax.set_ylabel(\"Densidade de Frequência\", fontsize=10)\n",
        "    sns.despine()\n",
        "\n",
        "# Ajustar layout\n",
        "plt.tight_layout()\n",
        "plt.show()"
      ],
