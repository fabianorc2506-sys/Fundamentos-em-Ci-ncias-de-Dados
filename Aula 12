        "# 1.1.1. **Gráfico de Pontos**"
      ],
      "metadata": {
        "id": "z-xD6f9Rr5d5"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Dados fornecidos\n",
        "numeros_irmaos = [0, 1, 2, 3, 4]  # Valores distintos\n",
        "frequencias = [2, 3, 3, 1, 1]     # Frequências\n",
        "\n",
        "# Preparar os dados para o gráfico de pontos\n",
        "pontos_x = []\n",
        "pontos_y = []\n",
        "for x, freq in zip(numeros_irmaos, frequencias):\n",
        "    for y in range(freq):\n",
        "        pontos_x.append(x)\n",
        "        pontos_y.append(y + 1)  # Os pontos são empilhados verticalmente\n",
        "\n",
        "# Criar o gráfico de pontos\n",
        "plt.figure(figsize=(8, 6))\n",
        "plt.scatter(pontos_x, pontos_y, color='blue', label='Estudantes')\n",
        "plt.xticks(numeros_irmaos)\n",
        "plt.xlabel('Número de Irmãos')\n",
        "plt.ylabel('Ocorrências')\n",
        "plt.title('Gráfico de Pontos - Número de Irmãos de Estudantes')\n",
        "plt.legend()\n",
        "plt.grid(True)\n",
        "plt.show()"
