    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import numpy as np\n",
        "from sklearn.linear_model import LinearRegression\n",
        "\n",
        "# Criando base com valores faltantes\n",
        "dados = pd.DataFrame({\n",
        "    'Nome': ['Ana', 'Bruno', 'Carlos', 'Daniela', 'Eduardo',\n",
        "             'Fernanda', 'Gustavo', 'Helena', 'Igor', 'Juliana'],\n",
        "    'Altura_cm': [162, np.nan, 175, 160, np.nan,\n",
        "                  168, 180, np.nan, 172, 165],\n",
        "    'Peso_kg': [55, 72, np.nan, 48, 70,\n",
        "                62, np.nan, 59, 75, np.nan],\n",
        "    'Sexo': ['F', 'M', 'M', 'F', np.nan,\n",
        "             'F', 'M', np.nan, 'M', 'F']\n",
        "})\n",
        "print(dados)"
