      "cell_type": "markdown",
      "source": [
        "# **Exemplo Sala de Aula**"
      ],
      "metadata": {
        "id": "-wXvq8j4HpEn"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import numpy as np\n",
        "from sklearn.linear_model import LinearRegression\n",
        "\n",
        "# Criando base com valores faltantes\n",
        "dados = pd.DataFrame({\n",
        "    'Nome': ['Ana', 'Bruno', 'Gustavo Henrique', 'Daniel', 'Pedro',\n",
        "             'Pietro', 'Gustavo Silva', 'Caio', 'Higor', 'Paulo'],\n",
        "    'Altura_cm': [170, np.nan, 180, 176, np.nan,\n",
        "                  183, 179, np.nan, 172, 184],\n",
        "    'calçado': ['38', '39', '42', '40', '44', '43', '42', '40', '39', '43']\n",
        "})\n",
        "\n",
        "# Convertendo a coluna 'calçado' para numérico\n",
        "dados['calçado'] = dados['calçado'].astype(int)\n",
        "\n",
        "print(\"Base original:\")\n",
        "print(dados)\n",
        "\n",
        "# Separar base com e sem missings em Altura_cm\n",
        "dados_treino = dados.dropna(subset=['calçado', 'Altura_cm'])  # onde ambos existem\n",
        "print(\"\\nBase para treino (sem missing em Altura_cm):\")\n",
        "print(dados_treino)\n",
        "\n",
        "# Ajuste do modelo\n",
        "modelo = LinearRegression()\n",
        "modelo.fit(dados_treino[['calçado']], dados_treino['Altura_cm'])\n",
        "\n",
        "# Exibindo os coeficientes\n",
        "print(f\"\\nCoeficiente angular (beta1): {modelo.coef_[0]:.4f}\")\n",
        "print(f\"Intercepto (beta0): {modelo.intercept_:.4f}\")\n",
        "\n",
        "# Prever os valores faltantes de Altura_cm\n",
        "faltantes = dados['Altura_cm'].isna()\n",
        "dados.loc[faltantes, 'Altura_cm'] = modelo.predict(dados.loc[faltantes, ['calçado']])\n",
        "\n",
        "# Mostrar resultado\n",
        "print(\"\\nBase com Altura_cm preenchida:\")\n",
        "print(dados)"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "7AMwPYZUHtit",
        "outputId": "1eb34f55-e5a9-46a0-bc61-1cb406808e5a"
      },
      "execution_count": 17,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "Base original:\n",
