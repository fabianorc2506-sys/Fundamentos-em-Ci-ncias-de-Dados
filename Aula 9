        "import numpy as np\n",
        "\n",
        "# Vetor de alturas de 100 indivíduos (em cm)\n",
        "alturas = np.array([150, 152, 154, 156, 158, 160, 162, 164,\n",
        "166, 168, 170, 172, 174, 176, 178, 180, 182, 184, 186,\n",
        "188, 190, 192, 194, 196, 198, 200, 202, 204, 206, 208,\n",
