        "\n",
        "# Dados: alturas e estratos (grupo etário)\n",
        "alturas = np.array([150, 152, 154, 156, 158, 160,\n",
        "                     162, 164, 166, 168, 170, 172, 174, 176, 178, 180,\n",
        "                     182, 184, 186, 188, 190, 192, 194, 196, 198, 200])\n",
        "\n",
        "estratos = np.array([1]*10 + [2]*10 + [3]*6)  # Verificando tamanhos compatíveis\n",
        "\n",
        "# Criando um DataFrame\n",
        "df = pd.DataFrame({\"Altura\": alturas, \"Estrato\": estratos})\n",
        "print(df)\n",
        "\n",
        "# Verificando se o DataFrame foi montado corretamente\n",
        "print(df.head())  # Exibe as primeiras linhas para conferir\n",
        "\n",
        "# Teste de amostragem estratificada\n",
        "proporcao = 0.3  # 30% da amostra\n",
        "\n",
        "try:\n",
        "    amostra = df.groupby(\"Estrato\", group_keys=False).apply(lambda x: x.sample(frac=proporcao))\n",
