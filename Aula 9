        "import numpy as np\n",
        "\n",
        "# Lista ordenada de alturas\n",
        "alturas = np.arange(150, 210, 2)  # 30 indivíduos\n",
        "\n",
        "# Tamanho da amostra\n",
        "n = 10\n",
        "\n",
        "# Intervalo de seleção\n",
        "k = len(alturas) // n\n",
        "\n",
        "# Escolhendo um ponto inicial aleatório\n",
        "start = np.random.randint(0, k)\n",
        "\n",
        "# Seleção sistemática\n",
        "amostra = alturas[start::k]\n",
        "\n",
        "# Exibindo a amostra\n",
        "print(amostra)\n"
      ],
