      "source": [
        "# **Gráfico de Pareto**"
      ],
      "metadata": {
        "id": "yfeEt85lM0hv"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Dados reais do primeiro turno das eleições presidenciais de 2022\n",
        "candidatos = ['Lula', 'Bolsonaro', 'Ciro Gomes', 'Simone Tebet']\n",
        "percentuais = [48.43, 43.20, 3.04, 4.16]  # Percentuais de votos válidos\n",
        "\n",
        "# Ordenar os dados por percentual\n",
        "candidatos_ordenados = [x for _, x in sorted(zip(percentuais, candidatos), reverse=True)]\n",
        "percentuais_ordenados = sorted(percentuais, reverse=True)\n",
        "\n",
        "# Calcular porcentagem acumulada\n",
        "porcentagem_acumulada = [sum(percentuais_ordenados[:i+1]) for i in range(len(percentuais_ordenados))]\n",
        "\n",
        "# Criar o gráfico de Pareto\n",
        "fig, ax1 = plt.subplots(figsize=(10, 6))\n",
        "\n",
        "# Barras para percentuais\n",
        "ax1.bar(candidatos_ordenados, percentuais_ordenados, color='blue', alpha=0.7)\n",
        "ax1.set_ylabel('Percentual de Votos (%)')\n",
        "ax1.set_title('Gráfico de Pareto - Eleições Presidenciais 2022 (1º Turno)')\n",
        "\n",
        "# Linha para porcentagem acumulada\n",
        "ax2 = ax1.twinx()\n",
        "ax2.plot(candidatos_ordenados, porcentagem_acumulada, color='red', marker='o', linestyle='-')\n",
        "ax2.set_ylabel('Porcentagem Acumulada (%)')\n",
        "\n",
        "# Ajustar layout e exibir o gráfico\n",
        "plt.tight_layout()\n",
        "plt.show()"
      ],
