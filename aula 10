          "metadata": {}
        }
      ],
      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Conjuntos de dados\n",
        "X = np.array([150, 155, 160, 165, 170, 175, 180, 210])\n",
        "Y = np.array([150, 155, 160, 165, 170, 175, 180])  # Sem o outlier 210\n",
        "\n",
        "# Ordenar os valores para exibição correta no eixo X\n",
        "X_sorted = np.sort(X)\n",
        "Y_sorted = np.sort(Y)\n",
        "\n",
        "# Criar índices para cada indivíduo\n",
        "indices_X = np.arange(1, len(X_sorted) + 1)\n",
        "indices_Y = np.arange(1, len(Y_sorted) + 1)\n",
        "\n",
        "# Calculando média e mediana\n",
        "mean_X, median_X = np.mean(X_sorted), np.median(X_sorted)\n",
        "mean_Y, median_Y = np.mean(Y_sorted), np.median(Y_sorted)\n",
        "\n",
        "# Criar gráficos lado a lado\n",
        "fig, axes = plt.subplots(1, 2, figsize=(10, 5))\n",
        "\n",
        "# Gráfico do conjunto X\n",
        "axes[0].scatter(indices_X, X_sorted, color='blue', s=70)\n",
        "axes[0].scatter([indices_X[np.argmin(np.abs(X_sorted - mean_X))],\n",
        "                 indices_X[np.argmin(np.abs(X_sorted - median_X))]],\n",
        "                [mean_X, median_X], color=['green', 'purple'], marker='X', s=100)\n",
        "axes[0].set_xticks(indices_X)\n",
        "axes[0].set_xticklabels([f\"Ind{idx}\" for idx in indices_X])  # Rótulos no eixo X\n",
        "axes[0].set_title(\"Conjunto X (com outlier)\")\n",
        "axes[0].set_xlabel(\"Indivíduos\")\n",
        "axes[0].set_ylabel(\"Altura (cm)\")\n",
        "axes[0].set_ylim(140, 220)\n",
        "\n",
        "# Gráfico do conjunto Y\n",
        "axes[1].scatter(indices_Y, Y_sorted, color='blue', s=70)\n",
        "axes[1].scatter([indices_Y[np.argmin(np.abs(Y_sorted - mean_Y))],\n",
        "                 indices_Y[np.argmin(np.abs(Y_sorted - median_Y))]],\n",
        "                [mean_Y, median_Y], color=['green', 'purple'], marker='X', s=100)\n",
        "axes[1].set_xticks(indices_Y)\n",
        "axes[1].set_xticklabels([f\"Ind{idx}\" for idx in indices_Y])  # Rótulos no eixo X\n",
        "axes[1].set_title(\"Conjunto Y (sem outlier)\")\n",
        "axes[1].set_xlabel(\"Indivíduos\")\n",
        "axes[1].set_ylabel(\"Altura (cm)\")\n",
        "axes[1].set_ylim(140, 200)\n",
        "\n",
        "plt.tight_layout()\n",
        "plt.show()"
      ]
