        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Dados reais aproximados das maiores torcidas no Brasil\n",
        "torcidas = ['Flamengo', 'Corinthians', 'Palmeiras', 'São Paulo', 'Vasco']\n",
        "percentuais = [24.8, 19.4, 8.1, 7.7, 4.8]  # Percentuais aproximados\n",
        "\n",
        "# Criar o gráfico de pizza\n",
        "fig, ax = plt.subplots(figsize=(8, 8))\n",
        "ax.pie(percentuais, labels=torcidas, autopct='%1.1f%%', startangle=90, colors=['red', 'black', 'green', 'blue', 'navy'])\n",
        "ax.set_title('Distribuição das Torcidas no Brasil')\n",
        "\n",
        "# Exibir o gráfico\n",
        "plt.show()\n"
