        "array1 = np.array([1, 2, 3])          # 1D\n",
        "array2 = np.array([[1, 2], [3, 4]])   # 2D\n",
        "array3 = np.array([[[1], [2]], [[3], [4]]])  # 3D\n",
        "\n",
        "# Verificando a dimensão (ndim)\n",
        "print(array1)\n",
        "print(array1.ndim)  # 1\n",
        "print(array2)\n",
        "print(array2.ndim)  # 2\n",
        "print(array3)\n",
