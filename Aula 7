        "# Escrevendo múltiplas linhas no arquivo\n",
        "linhas = ['Linha 1\\n', 'Linha 2\\n', 'Linha 3\\n']\n",
        "\n",
        "with open('exemplo.txt', 'w') as arquivo:\n",
        "    arquivo.writelines(linhas)\n",
        "\n",
        "print(\"Linhas escritas com sucesso!\")\n"
