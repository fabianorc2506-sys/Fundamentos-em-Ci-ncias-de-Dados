        "# **1.5. Escrita em Arquivos**"
      ],
      "metadata": {
        "id": "6r4sYfkV6_90"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "try:\n",
        "    f = open('construcao.txt', 'a')\n",
        "\n",
        "    while True:\n",
        "        linha = input()\n",
        "        if linha == '':\n",
        "            break\n",
        "        f.write(linha + \"\\n\")\n",
        "\n",
        "    f.close()\n",
        "\n",
        "except FileNotFoundError:\n",
        "    print(\"Arquivo não existe, criando um!\")\n",
        "    f = open('construcao.txt', 'w')"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "ZPUqlRvf7HHk",
        "outputId": "b8ccae80-cdd8-403c-c014-9cc4610ce903"
      },
      "execution_count": 13,
      "outputs": [
        {
          "name": "stdout",
          "output_type": "stream",
          "text": [
            "oi\n",
            "Como você está?\n",
            "\n"
          ]
        }
      ]
    },
