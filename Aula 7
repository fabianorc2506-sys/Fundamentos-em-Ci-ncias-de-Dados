        "# **1.4. Leitura de Arquivo com readline()**"
      ],
      "metadata": {
        "id": "fJmQ9Xhd6ynt"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "import os\n",
        "print(os.path.exists('meu_exemplo.txt'))  # Retorna True se o arquivo existe\n",
        "\n",
        "# Criando e escrevendo no arquivo\n",
        "with open('meu_exemplo.txt', 'w') as f:\n",
        "    f.write(\"Primeira linha\\nSegunda linha\\nTerceira linha\\n\")\n",
        "\n",
        "# Lendo uma linha por vez corretamente\n",
        "with open('meu_exemplo.txt', 'r') as f:\n",
        "    linha1 = f.readline().strip()  # Remove espaços e quebras de linha extras\n",
        "    linha2 = f.readline().strip()\n",
        "    linha3 = f.readline().strip()\n",
        "\n",
        "print(\"Linha 1:\", linha1)\n",
        "print(\"Linha 2:\", linha2)\n",
        "print(\"Linha 3:\", linha3)"
      ],
      "metadata": {
