      ],
      "metadata": {
        "id": "mmI6CJf-PD0o"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "# Copas do Mundo: Ano, Campeão, Sede\n",
        "copas_mundo = [[1930, \"Uruguai\", \"Uruguai\"],[1934, \"Itália\", \"Itália\"],[1938, \"Itália\", \"França\"],[1950, \"Uruguai\", \"Brasil\"],[1954, \"Alemanha Ocidental\", \"Suíça\"], [1958, \"Brasil\", \"Suécia\"],[1962, \"Brasil\", \"Chile\"],\n",
        " [1966, \"Inglaterra\", \"Inglaterra\"], [1970, \"Brasil\", \"México\"], [1974, \"Alemanha Ocidental\", \"Alemanha Ocidental\"],[1978, \"Argentina\", \"Argentina\"], [1982, \"Itália\", \"Espanha\"], [1986, \"Argentina\", \"México\"],\n",
        " [1990, \"Alemanha Ocidental\", \"Itália\"], [1994, \"Brasil\", \"EUA\"], [1998, \"França\", \"França\"], [2002, \"Brasil\", \"Coreia do Sul/Japão\"], [2006, \"Itália\", \"Alemanha\"], [2010, \"Espanha\", \"Africa do Sul\"],[2014, \"Alemanha\", \"Brasil\"],\n",
        " [2018, \"França\", \"Rússia\"], [2022, \"Argentina\", \"Catar\"]]\n",
        "\n",
        " # Contador para o Brasil\n",
        "contador_brasil = 0\n",
        "\n",
        "# Loop para passar por cada linha da tabela\n",
        "for copa in copas_mundo:\n",
        "    ano, campeao, sede = copa\n",
        "    print(copa)\n",
        "    if campeao == \"Brasil\":\n",
        "        contador_brasil += 1\n",
        "        print(f\"Ano: {ano}, Campeão: {campeao}, Sede: {sede}\")\n",
        "\n",
        "# Imprime o total de vezes que o Brasil foi campeão\n",
        "print(f\"O Brasil foi campeão {contador_brasil} vezes.\")\n",
        "\n"
      ],
