        "\n",
        "print(\"Primeiro elemento:\", lista[0])     # Acessando o primeiro elemento\n",
        "\n",
        "lista[1] = 10    # Modificando o segundo elemento\n",
        "print(\"Lista após modificação:\", lista)\n",
