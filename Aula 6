      ],
      "metadata": {
        "id": "3N0frFt_lfai"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "# Passo positivo\n",
        "passo_2 = lista[::2]\n",
        "\n",
        "# Passo negativo\n",
        "invertida = lista[::-1]\n",
        "\n",
        "# Fatiamento com passo específico\n",
        "passo_3 = lista[2:len(lista):3]\n",
        "\n",
        "print(\"Passo 2:\", passo_2)\n",
        "print(\"Lista invertida:\", invertida)\n",
        "print(\"Passo 3:\", passo_3)"
      ],
