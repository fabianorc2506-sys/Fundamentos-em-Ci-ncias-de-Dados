        "dicionario = {\"nome\": \"Ana\", \"idade\": 25, \"cidade\": \"São Paulo\"}\n",
        "print(\"Idade:\", dicionario[\"idade\"])\n",
        "\n",
        "dicionario[\"profissao\"] = \"Engenheira\"\n",
        "print(\"Dicionário após adição:\", dicionario)\n",
        "dicionario[\"cidade\"] = \"\"\n",
        "print(\"Dicionário após alteração:\", dicionario)\n",
        "\n",
        "del dicionario[\"cidade\"]\n",
        "print(\"Dicionário após remoção:\", dicionario)\n",
        "\n",
        "for chave, valor in dicionario.items():\n",
        "    print(f\"{chave}: {valor}\")\n",
        "\n",
        "# Verificando se um valor pertence ao dicionário\n",
        "print(\"Engenheira está no dicionário?\", \"Engenheira\" in dicionario.values())"
