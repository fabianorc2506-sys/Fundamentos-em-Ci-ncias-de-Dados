      ],
      "metadata": {
        "id": "DIMwEJNshQmu"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "# Solicita ao usuário um número inteiro n\n",
        "n = int(input(\"Digite um número inteiro: \"))\n",
        "\n",
        "# Inicializa a variável i para começar o laço de repetição\n",
        "i = 1\n",
        "\n",
        "# Exibe a tabuada de n de 1 até 10 utilizando o laço while\n",
        "print(f\"Tabuada do {n}:\")\n",
        "while i <= 10:\n",
        "    print(f\"{n} x {i} = {n * i}\")\n",
        "    i += 1"
      ],
