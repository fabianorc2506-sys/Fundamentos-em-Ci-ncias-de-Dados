        "print(\"Lista após adicionar 6:\", lista)\n",
        "\n",
        "lista.remove(3)  # Removendo um elemento específico\n",
        "print(\"Lista após remover 3:\", lista)\n",
        "\n",
        "lista.append('Corinthians')\n",
        "print(\"Lista após adicionar 'Corinthians':\", lista)"
