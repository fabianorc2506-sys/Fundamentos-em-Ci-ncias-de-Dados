        "# **1.8. Agrupamento com \\texttt{groupby} e uma chave**\n",
        "\n",
        "\n"
      ],
      "metadata": {
        "id": "UVIRycXreNo4"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import numpy as np\n",
        "df = pd.DataFrame({'chave1': ['a', 'a', 'b', 'b', 'a'],\n",
        "                   'chave2': ['um', 'dois', 'um', 'dois', 'um'],\n",
        "                   'data1': np.arange(2, 11, 2),\n",
        "                   'data2': np.linspace(0, 10, num = 5)})\n",
        "print(df)"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "p4Yu6wjqeXgX",
        "outputId": "f2bfeb50-b494-458a-d123-b734a4cf1221"
      },
      "execution_count": 27,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "  chave1 chave2  data1  data2\n",
            "0      a     um      2    0.0\n",
            "1      a   dois      4    2.5\n",
            "2      b     um      6    5.0\n",
            "3      b   dois      8    7.5\n",
            "4      a     um     10   10.0\n"
          ]
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "data1_agrup = df['data1'].groupby(df['chave1'])\n",
        "print(data1_agrup)\n",
        "#cria um objeto SeriesGroupBy, que apenas define como os dados devem ser agrupados, mas ainda não aplicou nenhuma operação como sum(), mean(),"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "1zeAt9lwidnX",
        "outputId": "d1a8b522-6a92-438e-9d8c-c36eabacd1f1"
      },
      "execution_count": 28,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "<pandas.core.groupby.generic.SeriesGroupBy object at 0x7f3a0e11fe00>\n"
          ]
        }
      ]
    },
    {
