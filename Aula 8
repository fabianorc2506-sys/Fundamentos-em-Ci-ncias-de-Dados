        "import pandas as pd\n",
        "\n",
        "# Criando o DataFrame com dados faltantes\n",
        "df = pd.DataFrame({\"vendedor\": [1, None, 3, 4, 5],\n",
        "                   \"numero_vendas\": [20.0, 30.0, None, 15.0, None],\n",
        "                   \"salario\": [2000, 3000, 1800, 1500, 3200]})\n",
        "\n",
        "# Substituindo valores NaN pela média da coluna\n",
        "df_preenchido = df.fillna(df.mean())\n",
        "print(df_preenchido)"
