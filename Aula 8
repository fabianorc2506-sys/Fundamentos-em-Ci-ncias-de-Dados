      ],
      "source": [
        "import numpy as np\n",
        "\n",
        "# Matrizes de exemplo\n",
        "A = np.array([[1, 2], [3, 4]])  # Dimensão (2, 2)\n",
        "B = np.array([[5, 6]])          # Dimensão (1, 2)\n",
        "C = np.array([[7, 8]])          # Dimensão (1, 2)\n",
        "D = np.array([[[9, 10]], [[11, 12]]])  # Dimensão (2, 1, 2)\n",
        "\n",
        "# Verificar dimensões\n",
        "print(\"Dimensão de A:\", A.shape)\n",
        "print(\"Dimensão de B:\", B.shape)\n",
        "print(\"Dimensão de C:\", C.shape)\n",
        "print(\"Dimensão de D:\", D.shape)\n",
        "\n",
        "# Empilhamento horizontal (hstack)\n",
        "# Para usar hstack, o número de linhas deve ser o mesmo.\n",
        "B_adjusted = np.array([[5], [6]])  # Ajusta B para ser compatível com A\n",
        "H = np.hstack((A, B_adjusted))  # Agora compatível\n",
        "print(\"hstack:\\n\", H)\n",
        "\n",
        "# Empilhamento vertical (vstack)\n",
        "# Para usar vstack, o número de colunas deve ser o mesmo.\n",
        "V = np.vstack((A, C))  # A e C já possuem 2 colunas, compatível\n",
        "print(\"vstack:\\n\", V)\n",
        "\n",
        "# Empilhamento na profundidade (dstack)\n",
        "# Para usar dstack, as matrizes devem ter as mesmas dimensões de linha e coluna.\n",
        "D_adjusted = np.hstack((D[:, :, 0], D[:, :, 0]))  # Ajusta D[:, :, 0] para ter 2 colunas como A\n",
        "D_new = np.dstack((A, D_adjusted))  # Agora compatível\n",
        "print(\"Dimensão ajustada de D[:, :, 0]:\", D_adjusted.shape)\n",
        "print(\"dstack:\\n\", D_new)"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "# Empilhamento vertical (vstack)\n",
        "# Para usar vstack, o número de colunas deve ser o mesmo.\n",
        "V = np.vstack((A, C))  # A e C já possuem 2 colunas, compatível\n",
        "print(\"vstack:\\n\", V)"
