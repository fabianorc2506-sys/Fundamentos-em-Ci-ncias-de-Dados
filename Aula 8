      "source": [
        "# **Agregação de Dados**"
      ],
      "metadata": {
        "id": "PIgr-ENDJUgz"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "import csv\n",
        "import pandas as pd\n",
        "\n",
        "# Nome do arquivo CSV existente\n",
        "arquivo_csv = \"personagens_terra_media.csv\"\n",
        "\n",
        "# Carregar o banco de dados existente\n",
        "df = pd.read_csv(arquivo_csv)\n",
        "\n",
        "# Novos personagens para adicionar\n",
        "novos_personagens = [\n",
        "    {\"Nome\": \"Legolas\", \"Raça\": \"Elfo\", \"Idade\": 2931},\n",
        "    {\"Nome\": \"Aragorn\", \"Raça\": \"Humano\", \"Idade\": 210},\n",
        "    {\"Nome\": \"Celeborn\", \"Raça\": \"Elfo\", \"Idade\": 6500},\n",
        "    {\"Nome\": \"Gil-Galad\", \"Raça\": \"Elfo\", \"Idade\": 7000},\n",
        "    {\"Nome\": \"Celebrimbor\", \"Raça\": \"Elfo\", \"Idade\": 4000},\n",
        "    {\"Nome\": \"Gimli\", \"Raça\": \"Anão\", \"Idade\": 140},\n",
        "    {\"Nome\": \"Frodo\", \"Raça\": \"Hobbit\", \"Idade\": 50},\n",
        "    {\"Nome\": \"Sam\", \"Raça\": \"Hobbit\", \"Idade\": 38},\n",
        "    {\"Nome\": \"Merry\", \"Raça\": \"Hobbit\", \"Idade\": 36},\n",
        "    {\"Nome\": \"Pippin\", \"Raça\": \"Hobbit\", \"Idade\": 28},\n",
        "    {\"Nome\": \"Isildur\", \"Raça\": \"Humano\", \"Idade\": 234}\n",
        "]\n",
        "\n",
        "# Criar um DataFrame com os novos personagens\n",
        "df_novos = pd.DataFrame(novos_personagens)\n",
        "\n",
        "# Concatenar os DataFrames\n",
        "df = pd.concat([df, df_novos], ignore_index=False)\n",
        "\n",
        "# Salvar no arquivo CSV atualizado\n",
        "df.to_csv(arquivo_csv, index=False, encoding='utf-8')\n",
        "\n",
        "print(f\"Arquivo '{arquivo_csv}' atualizado com sucesso!\")\n"
      ],
      "metadata": {
        "id": "7yC7jgmhWQfC",
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "outputId": "aa400d07-0f65-43a8-b875-32d0ef8df19c"
      },
      "execution_count": 16,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "Arquivo 'personagens_terra_media.csv' atualizado com sucesso!\n"
