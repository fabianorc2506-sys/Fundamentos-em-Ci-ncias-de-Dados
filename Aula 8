        "# **Operação com Matrizes**"
      ],
      "metadata": {
        "id": "Urags03TmI4w"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "\n",
        "A = np.array([[1, 2], [3, 4]])\n",
        "B = np.array([[5, 6], [7, 8]])\n",
        "\n",
        "# Multiplicação por escalar\n",
        "C = 2 * A\n",
        "\n",
        "# Soma de matrizes\n",
        "D = A + B\n",
        "\n",
        "# Produto matricial\n",
        "E = A @ B\n",
        "\n",
        "print(\"Escalar * Matriz:\\n\", C)\n",
        "print(\"Soma de Matrizes:\\n\", D)\n",
        "print(\"Produto Matricial:\\n\", E)"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
